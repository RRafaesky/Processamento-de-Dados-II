#inclui <stdio.h>

int  principal()  {
    int  número,  eu;

    // Solicita  ao  usuário  um  número  inteiro
    printf("Digite  um  número  inteiro:  ");
    scanf("%d",  número);

    // Exibe  a  tabela  do  número  fornecido  pelo  usuário  até  o  10
    printf("Tabuada  de  %d: \n ",  numero);
    para  (eu  =  1;  eu  <=  10;  eu++)  {
        printf("%d  x  %d  =  %d \n ",  numero,  i,  numero  *  i);
    }

    retornar  0;
}

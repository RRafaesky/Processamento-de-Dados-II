#include <stdio.h>

int main() {
    int numero, i;

    /* Solicita ao usuário um número inteiro*/
    printf("Digite um numero inteiro: ");
    scanf("%d", &numero);

    /* Exibe a tabuada do número fornecido pelo usuário até o 10*/
    printf("Tabuada de %d:\n", numero);
    for (i = 1; i <= 10; i++) {
        printf("%d x %d = %d\n", numero, i, numero * i);
    }

    return 0;
}
